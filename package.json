{
  "name": "sk8l",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "test:unit": "./node_modules/.bin/vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore",
    "serveprod": "pm2 serve --no-daemon dist 1337"
  },
  "dependencies": {
    "@achrinza/node-ipc": "^10.1.10",
    "@grpc/grpc-js": "^1.10.6",
    "@primer/octicons": "^19.7.0",
    "cronstrue": "^2.32.0",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.5.0",
    "luxon": "^3.4.3",
    "simple-statistics": "^7.8.3",
    "uplot": "^1.6.25",
    "vue": "^3.4.18",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@bufbuild/protobuf": "^1.7.2",
    "@connectrpc/connect": "^1.3.0",
    "@connectrpc/connect-web": "^1.3.0",
    "@connectrpc/protoc-gen-connect-es": "^1.3.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "jest": "^27.0.5",
    "sass": "^1.70.0",
    "vite": "^5.2.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/airbnb"
    ],
    "parserOptions": {},
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "args": "all"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        },
        "rules": {}
      }
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not ie 11"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "packageManager": "yarn@4.0.1"
}
